package MethodsWithOperators;

public class BitwiseBitCounter {
	public static int binaryconverter(int num) {
		String binaryconverted=Integer.toBinaryString(num);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=22;
		
		int binarydigits=Integer.parseInt(binaryconverted);
		System.out.println(binarydigits);
	}

}
